{
  "name": "video-streamer-server",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "typecheck": "./node_modules/.bin/tsc",
    "api": "./node_modules/.bin/tsc && node api/app.js",
    "api:blah": "node --inspect=0.0.0.0:5858 -r ts-node/register api/app.ts",
    "api:dev": "./node_modules/.bin/tsc-watch --onSuccess \"node --inspect=0.0.0.0:5858 api/app.js\"",
    "indexer": "./node_modules/.bin/tsc && node indexer/app.js",
    "indexer:dev": "./node_modules/.bin/nodemon --inspect=0.0.0.0 --watch --exec ./node_modules/.bin/ts-node indexer/app.ts",
    "converter": "./node_modules/.bin/tsc && node converter/app.js",
    "converter:dev": "./node_modules/.bin/tsc-watch --onSuccess \"node converter/app.js\"",
    "subtitler": "./node_modules/.bin/tsc && node subtitler/app.js",
    "subtitler:dev": "./node_modules/.bin/tsc-watch --onSuccess \"node subtitler/app.js\"",
    "metadata": "./node_modules/.bin/tsc && node metadata/app.js",
    "metadata:dev": "./node_modules/.bin/tsc-watch --onSuccess \"node metadata/app.js\"",
    "cast": "./node_modules/.bin/tsc && node cast.js",
    "cast-dev": "./node_modules/.bin/tsc-watch --onSuccess \"node ./cast.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrisharrington/video-streamer-server.git"
  },
  "author": "Chris Harrington",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chrisharrington/video-streamer-server/issues"
  },
  "homepage": "https://github.com/chrisharrington/video-streamer-server#readme",
  "_moduleAliases": {
    "@root": "./",
    "@api": "./api/",
    "@indexer": "./indexer/",
    "@converter": "./converter/",
    "@metadata": "./metadata/",
    "@subtitler": "./subtitler/"
  },
  "dependencies": {
    "amqplib": "^0.5.5",
    "body-parser": "^1.19.0",
    "castv2": "^0.1.10",
    "castv2-client": "^1.2.0",
    "chokidar": "^3.3.1",
    "cors": "^2.8.5",
    "dayjs": "^1.8.18",
    "express": "^4.16.3",
    "ffprobe": "^1.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "get-video-duration": "^1.0.3",
    "history": "^4.10.1",
    "install": "^0.12.1",
    "mdns": "^2.5.1",
    "module-alias": "^2.1.0",
    "mongodb": "^3.3.3",
    "node-fetch": "^2.6.0",
    "node-watch": "^0.6.3",
    "nodecastor": "^0.1.9",
    "npm": "^6.12.0",
    "opensubtitles-api": "^5.1.0",
    "react": "^16.12.0",
    "recursive-readdir": "^2.2.2",
    "srt-to-vtt": "^1.1.3",
    "uuid": "^3.3.2",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.8",
    "@types/express": "^4.16.0",
    "@types/mongodb": "^3.1.4",
    "@types/node": "^12.11.1",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "nodemon": "^1.19.4",
    "tsc-watch": "^4.0.0",
    "typescript": "^3.6.4"
  }
}
